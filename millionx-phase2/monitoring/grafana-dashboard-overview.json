{
  "dashboard": {
    "title": "MillionX Phase 2 - Overview",
    "tags": ["millionx", "phase2", "overview"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 0,
    "refresh": "30s",
    
    "panels": [
      {
        "id": 1,
        "title": "Scraper Success Rate (Last 24h)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(scraper_requests_total{status=\"success\"}[24h])) / sum(rate(scraper_requests_total[24h])) * 100",
            "legendFormat": "Success %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      
      {
        "id": 2,
        "title": "Messages Scraped (Last 1h)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(scraper_messages_scraped_total[1h]))",
            "legendFormat": "Messages"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      
      {
        "id": 3,
        "title": "Kafka Consumer Lag",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "kafka_consumergroup_lag{topic=~\"raw.*\"}",
            "legendFormat": "{{topic}} - {{consumergroup}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages Behind"},
          {"format": "short", "show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10000], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Kafka Consumer Lag",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      
      {
        "id": 4,
        "title": "Stream Processor DLQ Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(faust_processor_dlq_messages_total[5m]) * 100",
            "legendFormat": "{{processor}} DLQ %"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "DLQ Rate"},
          {"format": "short", "show": false}
        ],
        "thresholds": [
          {"value": 2, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      
      {
        "id": 5,
        "title": "Embedding Service Latency (P95/P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(faust_processor_processing_latency_seconds_bucket{processor=\"embedding-service\"}[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(faust_processor_processing_latency_seconds_bucket{processor=\"embedding-service\"}[5m]))",
            "legendFormat": "P99"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short", "show": false}
        ],
        "thresholds": [
          {"value": 0.2, "colorMode": "critical", "op": "gt", "fill": false, "line": true}
        ]
      },
      
      {
        "id": 6,
        "title": "Snowflake Daily Cost",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(increase(snowflake_credits_used_total[24h])) * 2.5",
            "legendFormat": "Cost USD"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 15, "color": "red"}
              ]
            }
          }
        }
      },
      
      {
        "id": 7,
        "title": "Messages Ingested to Snowflake",
        "type": "graph",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "rate(snowflake_rows_inserted_total[5m])",
            "legendFormat": "{{table}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Rows/sec"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 8,
        "title": "Privacy Shield Anonymization Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(faust_processor_messages_processed_total{processor=\"privacy-shield\",anonymized=\"true\"}[5m])) / sum(rate(faust_processor_messages_processed_total{processor=\"privacy-shield\"}[5m])) * 100",
            "legendFormat": "Anonymized %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      
      {
        "id": 9,
        "title": "Context Enricher - Weather API Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(redis_cache_hits_total{service=\"context-enricher\"}[5m]) / (rate(redis_cache_hits_total{service=\"context-enricher\"}[5m]) + rate(redis_cache_misses_total{service=\"context-enricher\"}[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 85, "color": "green"}
              ]
            }
          }
        }
      },
      
      {
        "id": 10,
        "title": "Product Category Detection Confidence",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(context_enricher_category_confidence_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellRadius": 0,
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 128
          }
        }
      },
      
      {
        "id": 11,
        "title": "Weaviate Vector Ingestion Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 24},
        "targets": [
          {
            "expr": "rate(weaviate_objects_total[5m])",
            "legendFormat": "{{collection}}"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Objects/sec"},
          {"format": "short", "show": false}
        ]
      },
      
      {
        "id": 12,
        "title": "System Health - Processors Up",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "up{job=~\".*processor.*\"}",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "instance": "Instance",
                "job": "Service",
                "Value": "Status"
              }
            }
          }
        ]
      },
      
      {
        "id": 13,
        "title": "Error Rate by Component",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{component}}"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.1, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        }
      ]
    },
    
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  
  "overwrite": true
}
